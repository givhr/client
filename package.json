{
  "name": "givhr",
  "description": "Givhr Client",
  "version": "1.0.0",
  "author": "Dallin Parker",
  "repository": {
    "type": "git",
    "url": "https://github.com/givhr/client"
  },
  "keywords": [
    "Human Resource",
    "Slack App"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prod": "npm run clean && gatsby build",
    "dev": "NODE_ENV=dev gatsby develop -p 3030 -o",
    "start": "gatsby serve -p 3030 -o",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "clean": "rm -rf public && rm -rf .cache",
    "type-check": "tsc --noEmit",
    "develop": "gatsby develop",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e:open": "start-server-and-test develop http://localhost:8000 cy:open",
    "e2e:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "lint": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' && npm run type-check"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "gatsby": "^2.3.14",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-create-client-paths": "^2.0.3",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-styled-components": "^3.0.1",
    "gatsby-plugin-typescript": "^2.0.12",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-sharp": "^2.1.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-pose": "^4.0.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "^7.0.6",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.0.3",
    "cypress": "^3.1.5",
    "docz": "^0.13.7",
    "gatsby-plugin-tslint": "0.0.2",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "start-server-and-test": "^1.7.13",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.2"
  }
}
