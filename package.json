{
  "name": "givhr",
  "description": "Givhr Client",
  "version": "1.0.0",
  "author": "Dallin Parker <dallin.r.parker@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/givhr/client"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prod": "npm run clean && gatsby build",
    "dev": "gatsby develop -p 3030 -o",
    "server": "gatsby serve -p 3030 -o",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' && npm run type-check",
    "type-check": "tsc --noEmit",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "dependencies": {
    "gatsby": "^2.0.117",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.8",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.1",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-styled-components": "^3.0.1",
    "gatsby-plugin-typescript": "^2.0.7",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/styled-components": "^4.0.3",
    "docz": "^0.12.9",
    "gatsby-plugin-tslint": "0.0.2",
    "husky": "^1.1.3",
    "prettier": "^1.15.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  },
  "keywords": [
    "Human Resource",
    "Slack App"
  ]
}
